#!/usr/bin/env node
var defaults = require('../.')
var cli = require('../lib/cli')
var util = require('util')

var opts = cli.parse()

if (opts) {
  defaults.apiDefaults(opts.t, opts.s, function (e, r) {
    if (e) {
      console.log(e)
      process.exit(1)
    } else {
      console.log(util.inspect(r, false, null))
    }
  })  
}

